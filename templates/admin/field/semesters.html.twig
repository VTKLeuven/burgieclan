{# @var ea \EasyCorp\Bundle\EasyAdminBundle\Context\AdminContext #}
{# @var field \EasyCorp\Bundle\EasyAdminBundle\Dto\FieldDto #}
{# @var entity \EasyCorp\Bundle\EasyAdminBundle\Dto\EntityDto #}
<i
    {% switch field.formattedValue|trans|raw %}
        {% case 'Semester 1' %} class="fa-solid fa-circle-half-stroke"
        {% case 'Semester 2' %} class="fa-solid fa-circle-half-stroke fa-rotate-180"
        {% case '' %} class="fa-regular fa-circle"
        {% default %} class="fa-solid fa-circle"
    {% endswitch %}
style="color: #74C0FC;"></i>
