{# @var ea \EasyCorp\Bundle\EasyAdminBundle\Context\AdminContext #}
{# @var field \EasyCorp\Bundle\EasyAdminBundle\Dto\FieldDto #}
{# @var entity \EasyCorp\Bundle\EasyAdminBundle\Dto\EntityDto #}
<i
    {% if field.formattedValue|trans|raw == 'Semester 1' %} class="fa-solid fa-circle-half-stroke"
    {% elseif field.formattedValue|trans|raw == 'Semester 2' %} class="fa-solid fa-circle-half-stroke fa-rotate-180"
    {% elseif field.formattedValue|trans|raw == '' %} class="fa-regular fa-circle"
    {% else %} class="fa-solid fa-circle"
    {% endif %}
style="color: #74C0FC;"></i>
